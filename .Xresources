!------------------------------------------------
! CURSOR SETTINGS
!------------------------------------------------

Xcursor.theme: Simple-and-Soft

!------------------------------------------------

!------------------------------------------------
! ST FONTS
!------------------------------------------------

st.alpha: 0.8
st.font: FiraCode-9
st.boldFont: FiraCode-9
st.termname: st-256color

!-----------------------------------------------

! ------------------------------------------------
! ROFI COLOR THEME
! ------------------------------------------------

rofi.font: Liberation Mono 9
rofi.width: 15
rofi.lines: 12
rofi.color-enabled: true
rofi.color-window: #000000, #000000, #000000
rofi.color-normal: #000000, #636363, #000000, #636363, #000000
rofi.color-active: #000000, #636363, #000000, #636363, #000000
rofi.color-urgent: #000000, #636363, #000000, #636363, #000000
